#paginate_category {
    margin: 10px 10px 2px;
}
.paginate_control{
}
.jPaginate{
    height:34px;
    position:relative;
    color:#a5a5a5;
    font-size:small;
	display: block;
	font-family: Arial;
}
.paginate ul {
    background: none repeat scroll 0 0 #FFFFFF;
    float: right;
    list-style: none outside none;
    margin: 10px 5px 10px 10px;
    padding: 0;
}

.paginate ul li {
    -moz-border-radius: 4px;
	-webkit-border-radius: 4px;
    border: 1px solid #D5D5D5;
    cursor: pointer;
	margin:0 1px!important;
}
.paginate ul li  span{
 padding:3px 5px 0px}

}
.paginate ul li:hover {
	background:#d5d5d5;
	opacity:0.6;
}
.paginate ul li a {
    display: block;
    font-weight: bold;
    outline: medium none;
    overflow: hidden;
    text-decoration: none;
}

.jPaginate a{
    line-height:15px;
    height:18px;
    cursor:pointer;
    padding:2px 5px;
    margin:2px;
    float:left;
}
.jPag-control-back{
	position:absolute;
	left:0px;
}
.jPag-control-front{
	position:absolute;
	top:0px;
}
.jPaginate span{
    cursor:pointer;
}
ul.jPag-pages{
    float:left;
    list-style-type:none;
    margin:0px 0px 0px 0px;
    padding:0px;
}
ul.jPag-pages li{
    display:inline;
    float:left;
    padding:0px;
    margin:0px;
}
ul.jPag-pages li a{
    float:left;
    padding:5px 5px 0px;
}
